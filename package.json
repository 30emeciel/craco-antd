{
  "name": "craco-antd",
  "version": "1.9.2",
  "description": "A craco plugin to use Ant Design with create-react-app ",
  "main": "lib/craco-antd.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint --fix lib",
    "format": "prettier --write **/*.js",
    "update_deps": "node scripts/update_deps"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/FormAPI/craco-antd.git"
  },
  "keywords": [
    "craco",
    "create-react-app",
    "React",
    "Ant Design",
    "UI library",
    "UI components"
  ],
  "author": "Nathan Broadbent <nathan@formapi.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormAPI/craco-antd/issues"
  },
  "homepage": "https://github.com/FormAPI/craco-antd#readme",
  "devDependencies": {
    "clone": "^2.1.2",
    "coveralls": "^3.0.2",
    "eslint": "5.6.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.3"
  },
  "dependencies": {
    "babel-plugin-import": "^1.11.0",
    "craco-less": "^1.5.1",
    "less-vars-to-js": "^1.3.0"
  },
  "peerDependencies": {
    "antd": ">= 3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
